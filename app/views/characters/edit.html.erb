<h1>Editing Character #<%= @character.id %></h1>

<form action="/characters/<%= @character.id %>/update">
  <div>
    <label for="show_id">show_id</label><br />
    <input id="show_id" name="show_id" value="<%= @character.show_id  %>" type="text" />
  </div>
  <div>
    <label for="actor_id">actor_id</label><br />
    <input id="actor_id" name="actor_id" value="<%= @character.actor_id  %>" type="text" />
  </div>
  <div>
    <input type="submit">
  </div>
</form>


